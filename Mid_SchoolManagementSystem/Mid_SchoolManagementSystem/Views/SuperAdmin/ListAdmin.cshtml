@model IEnumerable<Mid_SchoolManagementSystem.Models.DataAccess.admin>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>List Admin</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "CreateAdmin")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.adminconfirmpassword)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.adminname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.adminpassword)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.adminconfirmpassword)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.adminname)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.adminpassword)
                </td>
                <td>
                    @Html.ActionLink("Edit", "EditAdmin", new { id = item.id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.id }) |
                    @Html.ActionLink("Delete", "DeleteAdmin", new { id = item.id })
                </td>
            </tr>
        }

    </table>
</body>
</html>
