@model IEnumerable<Mid_SchoolManagementSystem.Models.DataAccess.subject>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ListSubject</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "CreateSubject")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.subjectname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.classid)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.subjectname)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.classid)
                </td>
                <td>
                    @Html.ActionLink("Edit", "EditSubject", new { id = item.subjectid }) |

                    @Html.ActionLink("Delete", "DeleteSubject", new { id = item.subjectid })
                </td>
            </tr>
        }

    </table>
</body>
</html>